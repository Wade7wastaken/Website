<configuration>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".7z" mimeType="application/x-7z-compressed" />
    </staticContent>

    <rewrite>
      <rules>
        <rule name="node">
          <match url="/*" />
          <action type="Rewrite" url="server.mjs" />
        </rule>
      </rules>
    </rewrite>

    <iisnode node_env="production" nodeProcessCommandLine="&quot;C:\Program Files\nodejs\node.exe&quot;" interceptor="&quot;%programfiles%\iisnode\interceptor.js&quot;" />

  </system.webServer>
  <location path="" overrideMode="Deny">
    <system.webServer>
      <handlers>
        <add name="iisnode" path="server.mjs" verb="*" modules="iisnode" />
      </handlers>
    </system.webServer>
  </location>
</configuration>
